updater_dir: /var/lib/asu/updater/
updater_threads: 4
worker:
    - /var/lib/asu/worker/

active_distros:
    - openwrt

# only imagebuilders of the following are downloaded
active_targets:
  - ath79/generic
  - ramips/mt7621
  - x86/64

distro_folder: /etc/asu/distributions/
download_folder: /var/cache/asu/download/
